<div class="container-fluid" ng-cloak ng-init="getSessionsWithVotes()" animate-with="fadeIn">
  <div class="col-sm-10">
    <h3><%= t('reports.labels.title.session_votes') %></h3><br>

    <table class="table table-hover table-sm" ng-if="sessions.length > 0">
      <tr ng-repeat="session in sessions | orderBy:'-votes'" class="reviewers-comments-summary" ng-show="isVisibleTheme(session.theme)">
        <td class="" width="20%">
          <div class="text-xs-center">
            <div class="votes-count center-block"><span>{{session.votes}}</span></div>
          </div>
        </td>
        <td class="session" width="80%">
          <h5>
            <a ng-href="#/sessions/show/{{session.id}}" target="_blank">{{session.title}}</a>
            <br><small><i>{{session.author}}</i></small>
          </h5>
        </td>
      </tr>
    </table>
  </div>
  <div ng-include="'templates/themes_list.html'"></div>
</div>
