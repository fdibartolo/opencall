<span class="clickable">
  <!-- <span class="fa fa-twitter text-twitter"></span> -->
  <span id="vote-btn" helpable="<%= t('sessions.help.vote') %>" data-index="{{$index}}" vote-action ng-if="session.voted || availableVotes > 0" fade-animation="true"></span>
  <span helpable="<%= t('sessions.help.fav') %>" data-index="{{$index}}" fav-action></span>
  <% if can? :review, SessionProposal %>
    <a id="review-btn" ng-href="#/sessions/review/{{session.id}}">
      <span class="fa fa-pencil-square-o text-success" title="<%= t('sessions.buttons.start_review') %>"></span>
    </a>
  <% end %>
</span>
